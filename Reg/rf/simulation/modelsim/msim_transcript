vsim work.rf
# vsim work.rf 
# Start time: 14:25:41 on Oct 23,2018
# Loading std.standard
# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading ieee.numeric_std(body)
# Loading work.rf(behave)
# Loading work.reg(description)
add wave -position insertpoint sim:/rf/*
force -freeze sim:/rf/rf_a1 000 0
force -freeze sim:/rf/rf_a2 000 0
force -freeze sim:/rf/rf_a3 110 0
force -freeze sim:/rf/rst 1 0
noforce sim:/rf/clk
force -freeze sim:/rf/clk 1 0, 0 {50 ps} -r 100
force -freeze sim:/rf/wr 1 0
force -freeze sim:/rf/rf_d3 1001001011101001 0
run
run
force -freeze sim:/rf/rst 0 0
run
run
run
force -freeze sim:/rf/rf_a3 011 0
force -freeze sim:/rf/rf_d3 1001011100010001 0
run
run
force -freeze sim:/rf/rf_a2 110 0
force -freeze sim:/rf/rf_a1 011 0
force -freeze sim:/rf/wr 0 0
run
run
